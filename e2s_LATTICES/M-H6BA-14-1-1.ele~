&divide_elements                                                               
        name = *,
        type = "edrift",
        maximum_length = 0.01,
&end  

&run_setup
    lattice	    = "M-H6BA-14-1-1.lte", 
    magnets         = "%s.mag",
    use_beamline    = "ringrf",
    p_central       = 5870.84,
    default_order   = 2
&end

&run_control &end

&twiss_output filename = "%s.twi",
        statistics=1, radiation_integrals=1,
&end

&rf_setup
        filename      = "%s.rf",
        harmonic      = 936, 
        total_voltage = 2.5e+6,
&end

&find_aperture
STRING output = %s.aper;
STRING search_output = %s.apso;
STRING boundary = %s.bnd;
STRING mode = "many-particle";
double xmin = -0.1;
double xmax = 0.1;
double ymin = 0.0;
double ymax = 0.1;
long nx = 21;
long ny = 11;
long n_splits = 0;
double split_fraction = 0.5;
double desired_resolution = 0.01;
long assume_nonincreasing = 0;
long verbosity = 0;
long offset_by_orbit = 0;
long n_lines = 11;
long optimization_mode = 0;
&end
